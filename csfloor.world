# csfloor.world
include "map.inc"
include "robot.inc"

# Configure the environment
floorplan
(
    bitmap "csfloor.png"
    size [43.780 56.040 3.000]

)

# Define the properties of the Stage window
window
(
    size [430 560]
    scale 100
)

# Spawn the robot
pioneer2dx
(
    name "robot"
    pose [-8 -14 0 0]
    odom_error [0 0 0]

)
